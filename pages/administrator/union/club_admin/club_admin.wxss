/* pages/administrator/club_manage/club_check/club_check.wxss */
.page{
    /* padding-bottom: 120px; */
}
.clubnameContainer{
    width: 90%;
    margin: 0 auto;
    padding: 20rpx 0;
    display: flex;
    align-items: center;
    justify-content: space-between;
}
.clubnameContainer image{
    width: 40rpx;
    height: 40rpx;
}
.clubname{
   font-size: 34rpx;
    margin-left: 20rpx;
}
/* 审核社团内容 */
.checkitem{    color: #333333;
 
    margin: 30rpx auto;
    width: 80%;
    padding: 30rpx;
    box-shadow:1px 1px 5px 1px rgba(159,188,217,0.22);
}
.checkitem .title text{
    font-size: 30rpx;
    color: #333333;
}
.checkitem .title{
    display: flex;
    justify-content: space-between;
    align-items: center;
}
.title image{
    width: 32rpx;
    height: 32rpx;
}
.checkitem .info{
    color: #cccccc;
    font-size: 28rpx;
    margin-top: 10rpx;
    display: flex;
    justify-content: space-between;
}
.scroll-view{
    padding-bottom: 50rpx;
}
/* 暂无数据 */
.nodataContainer{
    width: 100%;
    margin-top: 140rpx;
    display: flex;
    align-items: center;
    flex-direction: column;
}
.nodataContainer image{
    width: 85%;
}
.nodatatip{
    color: #cccccc;
    font-size: 34rpx;
    margin: 20rpx 0;
}
/* 管制社团 */
.adminContainer{
    padding-bottom: 120rpx;
}
/* 搜索 */
.searchContainer{
    width: 85%;
    background-color: #fafafd;
    margin: 20rpx auto;
    display:flex;
    padding: 20rpx 20rpx;
    border-radius: 10rpx;
}
.searchicon{
    width: 50rpx;
    height: 50rpx;
}
.searchicon>image{
    width: 40rpx;
    height: 40rpx;
}
.searchInput{
    width: 90%;
    font-size: 30rpx;
    color:#CDC7D3;
    display: flex;
    align-items: center;
}
/* 暂停招新的社团 */
.adminTitle{
    width: 85%;
    margin: 0 auto;
    font-size: 32rpx;
    color: #333333;
}
.clubItem{
    width: 80%;
    height: 90rpx;
    box-shadow:1px 1px 5px 1px rgba(159,188,217,0.22);

    color: #333333;
    border-radius: 10rpx;
    display: flex;
    justify-content: space-between;
    align-content: center;
    align-items: center;
    font-size: 30rpx;
    margin: 20rpx auto;
    padding: 15rpx 35rpx;
}
.clubItem>view{
    display: flex;
    justify-content: center;
    align-items: center;
}
/* 定期开放 */
/* .openPageContainer{

} */

.chooesDateContainer{
    width: 80%;
    /* height: 90rpx; */
    padding: 20rpx 30rpx;
    margin: 30rpx auto;
    display: flex;
    flex-direction: column;
    justify-content: center;
    box-shadow:1px 1px 5px 1px rgba(159,188,217,0.22);
    border-radius: 10rpx;
}
.tip{
    border-top: 1rpx #eeeeee solid;
    margin-top: 20rpx;
    padding-top: 10rpx;
    font-size: 28rpx;
    color: #cccccc;
}
.chooesDatetitle{
    display: flex;
    justify-content: space-between;
    align-items: center;
    margin-bottom: 10rpx;
}
.chooesDatetitle>view{
    font-size: 32rpx;
    color: #333333;
}
.date{
    color: #cccccc;
    font-size: 28rpx;
}
.chooesDatetitle>image{
    width: 45rpx;
    height: 45rpx;
}
.switchContainer{
    width: 80%;
    margin: 0 auto;
    padding: 20rpx 20rpx;
    display: flex;
    justify-content: space-between;

}
.switchContainer>text{
    font-size: 34rpx;
    color: #333333;
}
/* 版权声明 */
.copyright{
    padding: 30rpx 0;
    width: 100%;
    text-align: center;
    position: absolute;
    bottom: 50rpx;
}
  .copyright>view{
    color: #dbdbdb;
    font-size: 24rpx;
  }
  .calendar{
      z-index: 999;
      /* padding-bottom: 100rpx; */
  }