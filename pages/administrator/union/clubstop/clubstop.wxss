/* pages/administrator/union/clubstop/clubstop.wxss */
.page{
    width: 90%;
    margin: 0 auto;
    padding-bottom: 30rpx;
}
.title{ 
    font-size: 34rpx;
    padding: 10rpx 0;
    margin: 10rpx 0;
}

.formContainer{
    width: 100%;
    position: relative;
    bottom: 30rpx;
    margin: 0 auto;
}
.itemtitle{
    color: #cccccc;
    font-size: 34rpx;
    margin: 10rpx 0;
    width: 100%;
}
.itemContainer{
    margin: 20rpx 0;
}
.itemtext{
    background-color: #fafafd;
    border: 1rpx solid #cccccc;
    border-radius: 10rpx;
    min-height:55rpx;
    padding: 20rpx 20rpx;
    display: flex;
    align-items: center;
    margin: 0 auto;
    overflow-wrap: break-word;
}
.text{
    width: 100%;
    font-size: 32rpx;
    color: #333333;
}
.teacherContaier{
    margin-top: 10rpx;
}
.btContainer{
    display: flex;
    margin-top: 50rpx;
}
button::after{
    border: none;
 }
.bt{
    width: 30%;
    height: 80rpx;
    background-color: #FA5151;
    color: #ffffff;
    font-size: 30rpx;
}
.bthover{
    background-color: #e14949;
    
}
