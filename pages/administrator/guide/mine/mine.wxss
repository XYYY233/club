/* pages/administrator/guide/mine/mine.wxss */
.title{
    display: flex;
    padding: 10rpx 30rpx;
    align-items: center;
    justify-content: space-between;

}
.title image{
    width: 45rpx;
    height: 45rpx;
}
.title text{
    font-size: 32rpx;
    color: #333333;
    margin: 0 10rpx ;
}

.checkitem{    
    color: #333333;
 
    margin: 30rpx auto;
    width: 80%;
    padding: 30rpx;
    box-shadow:1px 1px 5px 1px rgba(159,188,217,0.22);
}
.checkitem .clubtitle text{
    font-size: 30rpx;
    color: #333333;
}
.checkitem .clubtitle{
    display: flex;
    justify-content: space-between;
    align-items: center;
}
.clubtitle image{
    width: 32rpx;
    height: 32rpx;
}
.checkitem .info{
    color: #cccccc;
    font-size: 28rpx;
    margin-top: 10rpx;
    display: flex;
    justify-content: space-between;
    width: 100%;
    word-break:break-all;
}
.container{
    padding-bottom: 120rpx;
}