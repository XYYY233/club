/* 修改状态 */
.revise{
    background-color: #ffffff;
    border: 1rpx solid #cccccc;
    border-radius: 10rpx;
    min-height: 65rpx;
    padding: 20rpx 20rpx;
    display: flex;
    align-items: center;
    /* width: 100%; */
    flex-direction: column;
    justify-content: center;
    margin: 0 auto;
    overflow-wrap: break-word;
}
/* == */
