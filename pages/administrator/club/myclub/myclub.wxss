/* pages/administrator/club/myclub/myclub.wxss */
.page{
    padding-bottom: 50rpx;
}
.clubnameContainer{
    width: 90%;
    margin: 0 auto;
    padding: 20rpx 0;
    display: flex;
    align-items: center;
    justify-content: space-between;
}
.clubnameContainer image{
    width: 40rpx;
    height: 40rpx;
}
.clubname{
    color: #333333;
    font-size: 34rpx;
    margin-left: 20rpx;
}
/* 我的会员 */
