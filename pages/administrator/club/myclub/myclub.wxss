}
button{
    display: flex;
    align-items: center;
    justify-content: center;
}
.grey{
    color: #cccccc;
}
picker{
    width: 100%;
}
.addicon{
    width: 32rpx;
    height: 32rpx;
    margin: 0 20rpx;
}
/* 指导老师 */
.teacherContainer{
    margin-top: 40rpx;
}
.teachertitleBox{
    display: flex;
    justify-content: space-between;
    align-items: center;
    height: 65rpx;
    /* width: 100%; */
}
.deletebt{
    background-color: #FA5151;
    width: 18%;
    height: 65rpx;
    font-size: 30rpx;
    line-height: 65rpx;
    color: #ffffff;
    border: none;
    margin: 0;
}
.deletebthover{
    background-color: #e14949;
}
/* 按钮 */
.btcontainer{
    width: 100%;
    margin: 50rpx 0;
    display: flex;
    justify-content: space-around;
    align-items: center;
}
.bt{
    background-color: #ffd73b;
    width: 80%;
    height: 100rpx;
    font-size: 34rpx;
    line-height: 100rpx;
    color: #333333;
    border: none;
}
.bthover{
    background-color: #E5C136;
}
.bthalf{
    background-color: #ffd73b;
    width: 35%;
    height: 100rpx;
    font-size: 34rpx;
    line-height: 100rpx;
    color: #333333;
    border: none;
}


/* 圆形按钮 */
.circlebtContainer{
    position: relative;
}
.circlebt{
    width: 130rpx;
    height: 130rpx;
    background-color: #ffd73b;
    border-radius: 50%;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    margin: 10rpx 0;

    -webkit-animation-name: fadeIn; /*动画名称*/
    -webkit-animation-duration: 0.5s; /*动画持续时间*/
    -webkit-animation-iteration-count: 1; /*动画次数*/
    -webkit-animation-delay: 0s; /*延迟时间*/

}
.bticon{
    width: 45rpx;
    height: 45rpx;
}
.bttext{
    font-size: 22rpx;
}

button::after{
    border: none;
}
 .cancelbt{
    background-color: #efefef;
}
.bthover{
    background-color: #E5C136;
}
.cancelbthover{
    background-color: #dcdcdc;
}



@-webkit-keyframes fadeIn {
    0% {
    opacity: 0; /*初始状态 透明度为0*/
    }
    50% {
    opacity: 0; /*中间状态 透明度为0*/
    }
    100% {
    opacity: 1; /*结尾状态 透明度为1*/
    }
}

.movable-area{
    /* pointer-events:none; */
    width: 100%;
  }
  .movable-view{ 
    pointer-events:auto;
    width: 130rpx;
    height: 300rpx;
    position: fixed;
    top: 150rpx;
    left:  calc(100% - 140rpx);
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
  }